{
  "name": "facebook-handshake",
  "version": "3.0.0",
  "description": "Seamlessly log into Facebook silently, with a popup or with a redirect on unsupported browsers (Chrome/iOS).",
  "author": "Federico Brigante <npm@bfred.it> (https://twitter.com/bfred_it)",
  "license": "MIT",
  "main": "dist/facebook-handshake.common-js.js",
  "jsnext:main": "dist/facebook-handshake.es-modules.js",
  "scripts": {
    "build:js": "bfred-npm-bundler facebook-handshake --byte-count",
    "build:size": "gzip-size dist/facebook-handshake.byte-count.js",
    "build": "npm-run-all --silent jsfix build:*",
    "jsfix": "xo --fix",
    "watch:build": "onchange 'index.js' --initial -- npm run build -- --continue-on-error",
    "watch": "npm-run-all --parallel --silent watch:*",
    "prepublish": "npm run build",
    "test": "xo; npm run build"
  },
  "devDependencies": {
    "babel-preset-es2015-rollup": "^1.1.1",
    "bfred-npm-bundler": "^5.0.0",
    "gzip-size-cli": "^1.0.0",
    "npm-run-all": "^1.7.0",
    "onchange": "^2.4.0",
    "xo": "^0.14.0"
  },
  "babel": {
    "presets": [
      "es2015-rollup"
    ]
  },
  "xo": {
    "esnext": true,
    "env": [
      "browser"
    ],
    "rules": {
      "comma-dangle": [2, "always-multiline"],
      "camelcase": 0,
      "prefer-template": 0
    }
  },
  "dependencies": {
    "console-class": "^2.1.0",
    "facebook-sdk-promise": "^1.2.0",
    "query-string": "^4.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bfred-it/facebook-handshake.git"
  },
  "bugs": {
    "url": "https://github.com/bfred-it/facebook-handshake/issues"
  },
  "homepage": "https://github.com/bfred-it/facebook-handshake#readme"
}
